generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Startup {
  id          String   @id @default(uuid())
  name        String
  description String
  rating      Float    @default(0)
  votes       Int      @default(0)
  createdAt   DateTime @default(now())
}
